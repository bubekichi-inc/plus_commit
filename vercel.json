{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "lp": false,
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "business.plus-commit.com"
        }
      ],
      "destination": "/business/:path*"
    }
  ]
}